{
  "name": "@ryanwersal/litra-cli",
  "version": "0.0.3",
  "description": "CLI for controlling Logi Litra",
  "keywords": [
    "logitech",
    "litra",
    "cli"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ryanwersal/litra",
  "bugs": {
    "url": "https://github.com/ryanwersal/litra/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanwersal/litra.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "litra": "bin/run"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "oclif.manifest.json",
    "bin",
    "lib"
  ],
  "scripts": {
    "build": "tsc && oclif manifest"
  },
  "dependencies": {
    "@ryanwersal/litra": "workspace:^",
    "oclif": "^3.0.1",
    "@oclif/core": "^1.9.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "conf": "^10.1.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.0",
    "@types/node": "^17.0.42",
    "@types/jest": "^28.1.1",
    "prettier": "^2.6.2",
    "eslint": "^8.17.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "typescript": "^4.7.3",
    "@jest/types": "^28.1.1",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "litra",
    "dirname": "litra",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1
    },
    "topicSeparator": " "
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  }
}
